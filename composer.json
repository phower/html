{
    "name": "phower/html",
    "description": "Some helper classes to generate HTML tags from PHP.",
    "homepage": "https://github.com/phower/html",
    "type": "library",
    "require": {
        "php": "^5.4",
        "phower/escaper": "^0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
